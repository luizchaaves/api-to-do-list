{
   "compilerOptions": {
     "target": "es2020",
     "module": "commonjs",
     "lib": [
       "dom",
       "es6",
       "es2017",
       "esnext.asynciterable",
     ],
     "sourceMap": true,
     "outDir": "./dist",
     "moduleResolution":"node",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "resolveJsonModule": true,
     "baseUrl": "./src",
     "plugins": [],
     "strict": false
   },
   "exclude": [
     "node_modules"
   ],
   "include": [
    "src"
   ]
 }